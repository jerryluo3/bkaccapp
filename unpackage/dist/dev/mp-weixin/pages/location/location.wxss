
.header_fixed{position:fixed;top:0;}
.header{width:100%;padding:10rpx 0;border-bottom:1rpx solid #b4b4b5;height:60rpx;line-height:60rpx;background:#fefefe;}
.header .kefu{text-align:center;}
.header .sendPos{padding:0 20rpx;}
.header .sendPos .sendPos_img,.header .sendPos .sendPos_img image{width:120rpx;height:27rpx;}
.header .sendPos .sendPos_img{text-align:center;padding-top:5rpx;display:inline-block;}
.header .sendPos .sendPos_img image{padding-top:5rpx;}
.header .sendPos .sendAddr{display: inline-block;color:#595757;font-size:28rpx;padding-left:8rpx;vertical-align: top;}
.header .sendPos .sendAddrDetail{display: inline-block;}
.header .sendPos .sendAddrDetail image{width:26rpx;height:15rpx;margin:5rpx;}
.header .search{width:240rpx;margin-right:20rpx;border:1rpx solid #878787;text-align:center;font-size:28rpx;border-radius:60rpx;color:#595757;}
.header .search image{width:40rpx;height:40rpx;display:inline-block;margin-top:8rpx; vertical-align:top;}
.h_80{width:100%;height:80rpx;display:block;}
page{background:#fff;}
.search{margin:20rpx;background:#f5f5f5;border-radius:60rpx;height:60rpx;line-height:60rpx;}
.search_img{width:40rpx;}
.search_img image{width:36rpx;height:36rpx;margin:12rpx 20rpx;}
.search_txt{padding:0 20rpx;line-height:60rpx;height:60rpx;}
.search_del{padding:0 10rpx;}
.search_del image{width:36rpx;height:36rpx;margin:12rpx;}
.dingwei{width:100%;}
.dingwei_title{background:#f5f5f5;padding:0 20rpx;height:60rpx;line-height:60rpx;color:#8d8d8d;}
.dingwei_info{padding:20rpx;color:#999;}
.tname{font-size:32rpx;font-weight:bold;}
.taddress{font-size:28rpx;}
.fujin{width:100%;}
.fujin_title{background:#f5f5f5;padding:0 20rpx;height:60rpx;line-height:60rpx;color:#8d8d8d;}
.fujin_list{padding:20rpx;}
.fujin_item{border-bottom:1rpx solid #eee;padding:20rpx 0;font-size:28rpx;color:#999;}
.mask{width:100%;height:100%;position:fixed;background-color:rgba(0,0,0,0.5);top:100rpx;z-index:10;}

